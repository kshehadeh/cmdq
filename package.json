{
  "name": "@kshehadeh/cmdq",
  "version": "0.0.8",
  "description": "A tool for chaining shell commands",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/kshehadeh/cmdq"
  },
  "exports": {
    ".": {
      "import": "./dist/bundle.esm.js",
      "require": "./dist/bundle.cjs.js"
    }
  },
  "types": "./dist/types",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "increment": "npm version patch --no-git-tag-version",
    "deploy": "./scripts/deploy.sh"
  },
  "keywords": [
    "shell",
    "command",
    "cmdq",
    "command queue",
    "shell command",
    "shell commands",
    "shell script",
    "shell scripts",
    "scripting",
    "bash"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Karim Shehadeh",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bun": "latest",
    "@types/jest": "^29.5.12",
    "bun": "^1.1.24",
    "jest": "^29.7.0",
    "rollup": "^4.21.0",
    "ts-jest": "^29.2.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "module": "index.ts"
}
